# Platform
CONFIG_MACH_XIAOMI_APOLLO=y

# Touchscreen
CONFIG_TOUCHSCREEN_NT36xxx_HOSTDL_SPI=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y

# Fingerprint
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_FPC=y
CONFIG_FINGERPRINT_GOODIX=y

# Power
CONFIG_BQ2597X_CHARGE_PUMP=y
CONFIG_BATT_VERIFY_BY_DS28E16=y
CONFIG_ONEWIRE_GPIO=y

# Ultrasound Proximity
CONFIG_US_PROXIMITY=y

# Vibrator
CONFIG_INPUT_AW8697_HAPTIC=y

# Camera
CONFIG_TECHPACK_CAMERA_XIAOMI=y

# Thermals
CONFIG_QTI_THERMAL_LIMITS_DCVS=y

# Mitigations
CONFIG_CMDLINE="cgroup_disable=pressure mitigations=off ramoops_memreserve=4M"

# KernelSU
CONFIG_KSU=y
# CONFIG_KPROBES is not set

# Disable Audit
# CONFIG_AUDIT is not set
# CONFIG_HAVE_ARCH_AUDITSYSCALL is not set
# CONFIG_AUDITSYSCALL is not set
# CONFIG_AUDIT_WATCH is not set
# CONFIG_AUDIT_TREE is not set
# CONFIG_AUDIT_GENERIC is not set
# CONFIG_AUDIT_ARCH_COMPAT_GENERIC is not set
# CONFIG_AUDIT_COMPAT_GENERIC is not set